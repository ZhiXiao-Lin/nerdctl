{
  "name": "nerdctl",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Node wrapper for nerdctl",
  "repository": "git@github.com:ZhiXiao-Lin/nerdctl.git",
  "author": "RoyLin <roylin1996@163.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc && tsc-alias",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "start": "cross-env NODE_ENV=development ts-node --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "cac": "^6.7.12",
    "ts-node-dev": "^1.1.8",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/node": "^17.0.23",
    "@types/shelljs": "^0.8.11",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.6.5",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "shelljs": "^0.8.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
