{
  "name": "nerdctl",
  "version": "0.1.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Node wrapper for nerdctl",
  "repository": "git@github.com:ZhiXiao-Lin/nerdctl.git",
  "author": "RoyLin <roylin1996@163.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc && tsc-alias",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "start": "cross-env NODE_ENV=development ts-node --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "prepublish": "yarn build",
    "docs": "typedoc src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "lodash": "^4.17.21",
    "node-fetch": "2",
    "shelljs": "^0.8.5",
    "ts-node-dev": "^1.1.8",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "^2.6.1",
    "@types/shelljs": "^0.8.11",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^16.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "tsc-alias": "^1.6.5",
    "tsconfig-paths": "^3.14.1",
    "typedoc": "^0.22.14",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "shelljs": "^0.8.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
